DATABASE_URL=postgres://user:password@localhost:5432/anyskin
PAYLOAD_SECRET=YOUR_SECRET_HERE

# Storage: "local" (default) or "s3"
STORAGE_ADAPTER=local

# S3 / S3-compatible storage (only used when STORAGE_ADAPTER=s3)
S3_BUCKET=anyskin-media
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_REGION=us-east-1
# For non-AWS services (MinIO, etc.), set the endpoint URL:
# S3_ENDPOINT=http://localhost:9000
